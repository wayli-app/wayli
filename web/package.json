{
	"name": "web",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"test:accessibility": "vitest --run tests/accessibility/",
		"test:accessibility:watch": "vitest tests/accessibility/",
		"test:integration": "vitest --run tests/integration/",
		"test:integration:watch": "vitest tests/integration/",
		"test:coverage": "vitest --run --coverage",
		"test:performance": "vitest --run tests/performance/",
		"test:security": "vitest --run tests/security/",
		"test:realtime": "bun run src/scripts/test-realtime.ts",
		"test:jobs": "bun run src/scripts/test-job-creation.ts",
		"test:live-updates": "bun run src/scripts/test-live-updates.ts",
		"test:enhanced-jobs": "bun run src/scripts/test-enhanced-jobs.ts",
		"test:totp": "bun run src/scripts/test-totp.ts",
		"test:cache": "bun run src/scripts/test-cache.ts",
		"test:poi-visits": "bun run src/scripts/test-poi-visit-detection.ts",
		"worker": "bun run src/scripts/worker.ts",
		"worker:start": "bun run src/scripts/worker.ts",
		"worker:dev": "bun --watch src/scripts/worker.ts",
		"workers": "bun run src/scripts/worker-manager.ts",
		"workers:start": "bun run src/scripts/worker-manager.ts",
		"workers:dev": "bun --watch src/scripts/worker-manager.ts",
		"check:separation": "node scripts/check-client-server-separation.js"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/leaflet.markercluster": "^1.5.5",
		"@types/qrcode": "^1.5.5",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-svelte": "^3.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"globals": "^16.0.0",
		"jsdom": "^26.1.0",
		"lucide-static": "^0.525.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.0.0",
		"@melt-ui/svelte": "^0.86.6",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.50.0",
		"@svelte-plugins/datepicker": "^1.0.11",
		"@turf/turf": "^7.2.0",
		"@types/jszip": "^3.4.0",
		"@types/leaflet": "^1.9.18",
		"@types/lodash-es": "^4.17.12",
		"@types/pg": "^8.15.4",
		"@types/uuid": "^10.0.0",
		"clsx": "^2.1.0",
		"date-fns": "^4.1.0",
		"dotenv": "^16.5.0",
		"express-rate-limit": "^7.5.1",
		"fast-xml-parser": "^5.2.5",
		"jszip": "^3.10.1",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"lodash-es": "^4.17.21",
		"lucide-react": "^0.525.0",
		"lucide-svelte": "^0.344.0",
		"otplib": "^12.0.1",
		"pg": "^8.16.3",
		"postgres": "^3.4.7",
		"qrcode": "^1.5.4",
		"supabase": "^2.30.4",
		"svelte-sonner": "^1.0.5",
		"tailwind-merge": "^2.6.0",
		"uuid": "^11.1.0",
		"zod": "^4.0.5"
	}
}
